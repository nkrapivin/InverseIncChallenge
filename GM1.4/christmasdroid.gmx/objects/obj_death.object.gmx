    text_pos += text_spd;
    
    // pause at newlines with no space.
    if (string_char_at(text_orig, floor(text_pos) - 1) != " " &amp;&amp; string_char_at(text_orig, floor(text_pos)) == "#") {
        advance = false;
        //text_pos += 1;
        alarm[0] = room_speed / 3;
    }
    
    if (keyboard_check_pressed(vk_shift)) text_spd *= 5;
}

